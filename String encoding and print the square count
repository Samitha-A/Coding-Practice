#include<stdio.h>
#include<string.h>
int main(){
    char str[100];
    scanf("%[^\n]",str);
    getchar();
    int len=strlen(str);
    int freq[100]={0}, count=0, sum=0;
    for(int i=0; i<len; i++){
        count=1;
        if(freq[i]!=1){
            for(int j=i+1; j<len; j++){
                if(str[i]==str[j]){
                    count++;
                    freq[j]=1;
                }
            }
        }
        if(freq[i]==0){
            sum=sum+count;
            printf("%d",count);
        }
    }
    printf("\n%d",sum*sum);
}
