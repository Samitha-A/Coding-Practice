#include<stdio.h>
#include<ctype.h>
int main(){
    char str[100];
    scanf("%[^\n]",str);
    int i=0,sum=0;
    while(str[i]!='\0'){
        int num=0;
        while(isdigit(str[i])){
            num=num*10+(str[i]-'0');
            i++;
            if(!isdigit(str[i]))
            sum+=num;
        }
        i++;
    }
    printf("%d",sum);
}
